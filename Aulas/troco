notas_e_moedas = [200, 100, 50, 20, 10, 5, 2, 1, 0.50, 0.25, 0.10, 0.05, 0.01]

v_recebido=float(input("Qual o valor recebido:"))
v_a_receber=float(input("Qual o valor a receber:"))
                  
troco = v_recebido-v_a_receber

for grana in notas_e_moedas:
    n_de_itens = 0
    while grana <= troco:
        n_de_itens += 1
        troco -= grana
    if n_de_itens > 0:
        print(f"{n_de_itens} itens de {grana}")

